{
  "name": "@daitanjs/development",
  "version": "1.0.0",
  "description": "Development utilities for environment management, Google APIs, and system information.",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "sideEffects": false,
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "scripts": {
    "clean": "rimraf dist",
    "build:esm": "babel src --out-dir dist --extensions \".js\" --source-maps inline --env-name esm",
    "build:cjs": "babel src --out-dir dist --extensions \".js\" --source-maps inline --env-name cjs --out-file-extension .cjs",
    "build": "npm run clean && npm run build:esm && npm run build:cjs",
    "test": "echo \"Error: no test specified\" && exit 0",
    "prepare": "npm run build"
  },
  "files": [
    "dist/**/*"
  ],
  "keywords": [
    "development",
    "googleapis",
    "systeminformation",
    "winston",
    "utilities"
  ],
  "author": "Buck Miller <haelpers@gmail.com>",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/daitandojo/@daitanjs"
  },
  "peerDependencies": {
    "lodash": "^4.17.21"
  },
  "dependencies": {
    "googleapis": "latest",
    "systeminformation": "latest",
    "winston": "latest",
    "dotenv": "latest"
  },
  "publishConfig": {
    "access": "public"
  }
}
