{
  "name": "@daitanjs/web",
  "version": "1.2.1",
  "description": "An intelligent library for robust web scraping, content extraction, and Google Custom Search, featuring automated strategy selection and a clean text output mode.",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "sideEffects": false,
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "files": [
    "dist/**/*",
    "src/**/*"
  ],
  "scripts": {
    "clean": "rimraf dist",
    "build:esm": "babel src --out-dir dist --extensions \".js\" --source-maps inline --env-name esm",
    "build:cjs": "babel src --out-dir dist --extensions \".js\" --source-maps inline --env-name cjs --out-file-extension .cjs",
    "build": "npm run clean && npm run build:esm && npm run build:cjs",
    "test": "jest --passWithNoTests --coverage",
    "prepare": "npm run build"
  },
  "keywords": [
    "web scraping",
    "content extraction",
    "Google Custom Search",
    "puppeteer",
    "playwright",
    "jsdom",
    "cheerio",
    "web agent",
    "reader mode",
    "data extraction",
    "automation"
  ],
  "author": "Buck Miller <haelpers@gmail.com>",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/daitandojo/@daitanjs",
    "directory": "packages/web"
  },
  "peerDependencies": {},
  "dependencies": {
    "@daitanjs/config": "^1.0.0",
    "@daitanjs/development": "^1.0.0",
    "@daitanjs/error": "^1.0.1",
    "@daitanjs/utilities": "^1.0.0",
    "@daitanjs/apiqueries": "^1.0.3",
    "@daitanjs/validation": "^1.0.1",
    "jsdom": "^24.1.0",
    "puppeteer-extra": "^3.3.6",
    "puppeteer-extra-plugin-stealth": "^2.11.2",
    "playwright": "^1.44.1",
    "cheerio": "^1.0.0-rc.12",
    "node-fetch": "^3.3.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.24.7",
    "@babel/core": "^7.24.7",
    "@babel/plugin-transform-runtime": "^7.24.7",
    "@babel/preset-env": "^7.24.7",
    "babel-jest": "^29.7.0",
    "jest": "^29.7.0",
    "rimraf": "^5.0.7"
  },
  "jest": {
    "transform": {
      "^.+\\\\.jsx?$": "babel-jest"
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "homepage": "https://github.com/daitandojo/@daitanjs/tree/main/packages/web#readme"
}
