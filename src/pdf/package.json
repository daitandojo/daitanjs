  "dependencies": {
    "@daitanjs/development": "^1.0.0",
    "@daitanjs/error": "^1.0.1",
    "puppeteer": "^22.10.0",
    "pdf-lib": "^1.17.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.24.7",
    "@babel/core": "^7.24.7",
    "@babel/plugin-transform-runtime": "^7.24.7",
