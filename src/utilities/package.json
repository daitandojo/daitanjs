{
  "name": "@daitanjs/utilities",
  "version": "1.0.0",
  "description": "A utility library for retry mechanisms, batch processing, and error handling.",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "sideEffects": false,
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "scripts": {
    "clean": "rimraf dist",
    "build:esm": "babel src --out-dir dist --extensions \".js\" --source-maps inline --env-name esm",
    "build:cjs": "babel src --out-dir dist --extensions \".js\" --source-maps inline --env-name cjs --out-file-extension .cjs",
    "build": "npm run clean && npm run build:esm && npm run build:cjs",
    "test": "echo \"Error: no test specified\" && exit 0",
    "prepare": "npm run build"
  },
  "files": [
    "dist/**/*"
  ],
  "keywords": [
    "utilities",
    "retry",
    "batch processing",
    "error handling"
  ],
  "author": "Buck Miller <haelpers@gmail.com>",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/daitandojo/@daitanjs"
  },
  "peerDependencies": {},
  "dependencies": {},
  "publishConfig": {
    "access": "public"
  }
}
