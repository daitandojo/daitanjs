  },
  "dependencies": {
    "@tensorflow/tfjs-node": "^4.20.0",
    "@daitanjs/data": "^1.0.0",
    "@daitanjs/development": "^1.0.0",
    "@daitanjs/error": "^1.0.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.24.7",
