{
  "name": "@daitanjs/office",
  "version": "1.0.1",
  "description": "A library for creating and managing Excel, PowerPoint, and Word documents.",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "sideEffects": false,
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "files": [
    "dist/**/*",
    "src/**/*"
  ],
  "scripts": {
    "clean": "rimraf dist",
    "build:esm": "babel src --out-dir dist --extensions \".js\" --source-maps inline --env-name esm",
    "build:cjs": "babel src --out-dir dist --extensions \".js\" --source-maps inline --env-name cjs --out-file-extension .cjs",
    "build": "npm run clean && npm run build:esm && npm run build:cjs",
    "test": "echo \"Error: no test specified\" && exit 0",
    "prepare": "npm run build"
  },
  "keywords": [
    "office",
    "Excel",
    "PowerPoint",
    "Word",
    "documents",
    "charts",
    "PptxGenJS",
    "docx",
    "ExcelJS",
    "officegen",
    "mammoth"
  ],
  "author": "Buck Miller <haelpers@gmail.com>",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/daitandojo/@daitanjs",
    "directory": "packages/office"
  },
  "peerDependencies": {},
  "dependencies": {
    "@daitanjs/development": "^1.0.0",
    "@daitanjs/error": "^1.0.1",
    "exceljs": "^4.4.0",
    "docx": "^8.5.0",
    "pptxgenjs": "^3.12.0",
    "pptx-parser": "^1.1.7-beta.9",
    "mammoth": "^1.7.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.24.7",
    "@babel/core": "^7.24.7",
    "@babel/plugin-transform-runtime": "^7.24.7",
    "@babel/preset-env": "^7.24.7",
    "rimraf": "^5.0.7"
  },
  "publishConfig": {
    "access": "public"
  },
  "homepage": "https://github.com/daitandojo/@daitanjs/tree/main/packages/office#readme"
}