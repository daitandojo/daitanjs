{
  "name": "@daitanjs/data",
  "version": "1.0.1",
  "description": "Data handling utilities for caching, CSV processing, and database management.",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "sideEffects": false,
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "clean": "rimraf dist",
    "build:esm": "babel src --out-dir dist --extensions \".js\" --source-maps inline --env-name esm",
    "build:cjs": "babel src --out-dir dist --extensions \".js\" --source-maps inline --env-name cjs --out-file-extension .cjs",
    "build": "npm run clean && npm run build:esm && npm run build:cjs",
    "test": "echo \"Error: no test specified\" && exit 0",
    "prepare": "npm run build"
  },
  "files": [
    "dist/**/*"
  ],
  "keywords": [
    "data",
    "caching",
    "csv",
    "database",
    "mongodb",
    "mongoose"
  ],
  "author": "Buck Miller <haelpers@gmail.com>",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/daitandojo/@daitanjs"
  },
  "peerDependencies": {
    "lodash": "^4.17.21",
    "uuid": ">=9.0.0"
  },
  "dependencies": {
    "@daitanjs/development": "^1.0.0",
    "@daitanjs/error": "^1.0.1",
    "@daitanjs/utilities": "^1.0.0",
    "aws4": "^1.13.2",
    "axios": "latest",
    "csv-parse": "latest",
    "csv-stringify": "latest",
    "mongodb": "latest",
    "mongoose": "latest",
    "mysql2": "^3.10.0",
    "node-cache": "latest",
    "passport-local-mongoose": "latest",
    "winston": "latest"
  },
  "publishConfig": {
    "access": "public"
  }
}
