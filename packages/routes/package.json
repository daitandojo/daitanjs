{
  "name": "@daitanjs/routes",
  "version": "1.0.4",
  "description": "Reusable Next.js App Router API route handlers for the DaitanJS ecosystem.",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "sideEffects": false,
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "clean": "rimraf dist",
    "build": "babel src --out-dir dist --extensions \".js\" --source-maps inline",
    "watch": "babel src --out-dir dist --extensions \".js\" --source-maps inline --watch",
    "test": "echo \"Error: no test specified for @daitanjs/routes\" && exit 0"
  },
  "keywords": [
    "daitanjs",
    "nextjs",
    "routes",
    "api",
    "route handlers"
  ],
  "author": "Buck Miller <haelpers@gmail.com>",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/daitandojo/@daitanjs",
    "directory": "packages/routes"
  },
  "dependencies": {
    "@daitanjs/authentication": "^1.0.1",
    "@daitanjs/config": "^1.0.0",
    "@daitanjs/data": "^1.0.0",
    "@daitanjs/development": "^1.0.0",
    "@daitanjs/error": "^1.0.1",
    "@daitanjs/users": "^1.0.0",
    "@daitanjs/middleware": "^1.0.0",
    "@daitanjs/geo": "^1.0.1",
    "@daitanjs/speech": "^1.0.1",
    "@daitanjs/intelligence": "^1.2.0",
    "@daitanjs/senses": "^1.0.1",
    "@daitanjs/payments": "^1.0.1",
    "@daitanjs/queues": "^1.0.0",
    "@daitanjs/media": "^1.0.3",
    "next": "^14.2.3"
  },
  "publishConfig": {
    "access": "public"
  },
  "homepage": "https://github.com/daitandojo/@daitanjs/tree/main/packages/routes#readme"
}
